#include<iostream>
#include<cstdio>
#include<cmath>
#include<algorithm> 
#define ll long long 
using namespace std; 
int t; 
ll a,b; 

int main(){ 	
  scanf("%d",&t); 	
  for(int i=1;i<=t;++i){ 		
    scanf("%lld%lld",&a,&b); 		
    ll temp=__gcd(a,b); 		
    if(temp==a&&a*b/temp==b) 			
      printf("%lld %lld\n",a,b); 		
    else 
      printf("%d\n",-1);
    } 	
  return 0; 
}
